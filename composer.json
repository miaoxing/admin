{
  "name": "miaoxing/admin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Admin Plugin",
  "keywords": [
    "miaoxing",
    "admin"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "dev-master@dev",
    "miaoxing/coding-standards": "^5@beta"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Admin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Admin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
