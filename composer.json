{
  "name": "miaoxing/admin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Admin Plugin",
  "keywords": [
    "miaoxing",
    "admin"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.7.1",
    "miaoxing/file": "^0.3.14"
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Admin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Admin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev",
      "wei/wei": "twinh/wei as 0.15.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.11.x-dev",
      "miaoxing/services": "miaoxing/services as 0.7.x-dev",
      "miaoxing/app": "miaoxing/app as 0.7.x-dev"
    }
  }
}
