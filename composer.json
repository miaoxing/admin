{
  "name": "miaoxing/admin",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Admin Plugin",
  "keywords": [
    "miaoxing",
    "admin"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.2.10",
    "miaoxing/file": "^0.1.5"
  },
  "require-dev": {
    "miaoxing/dev": "^6.3.2"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Admin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Admin\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-types": [
      "miaoxing-plugin"
    ],
    "installer-paths": {
      "plugins/{$name}/": [
        "type:miaoxing-plugin"
      ]
    }
  }
}
